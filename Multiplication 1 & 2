#Multiplication 1
def mul (a,b):
    if a < b:
        product = 0
        while a > 0:
            product = b + product
            a = a - 1
        return product
    if a > b:
        product = 0
        while b > 0:
            product = a + product
            b = b- 1
        return product

#Multiplication 2
def emul (a,b):
    product = 0
    while b > 0:
        if b%2 != 0:
            product = a + product
        a = a*2
        b = b//2
    return product 
